{
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": true,
    "lib": [
      "dom.iterable",
      "dom",
      "es6"
    ],
    "module": "ES6",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noImplicitThis": false,
    "outDir": "build/types",
    "preserveConstEnums": true,
    "removeComments": true,
    "resolveJsonModule": false,
    "sourceMap": true,
    "strict": true,
    "target": "ES6",
  },
  "include": [
    "stack-up/*",
  ],
  "exclude": [
    "node_modules",
    "source"
  ]
}