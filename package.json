{
  "name": "@nekobird/stack-up",
  "version": "0.0.1",
  "description": "An efficient and optimized way of stacking fixed width, variable height elements.",
  "homepage": "https://github.com/nekobird/stack-up#readme",
  "bugs": {
    "url": "https://github.com/nekobird/stack-up/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nekobird/stack-up.git"
  },
  "license": "MIT",
  "author": "Andrew Prasetya",
  "files": [
    "build/**/*"
  ],
  "main": "build/stack-up.js",
  "types": "build/stack-up.d.ts",
  "scripts": {
    "build": "rm -r build; tsc -t es6 --emitDeclarationOnly; parcel build ./stack-up/stack-up.ts --out-dir build",
    "build-docs": "rm -r ./docs; parcel build ./source/index.html --out-dir docs --public-url ./",
    "clean": "rm -r dist; rm -r build; rm -r .cache",
    "prepare": "npm run build",
    "start": "parcel ./source/index.html",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@nekobird/rocket": "0.0.125"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/plugin-transform-typescript": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "autoprefixer": "^9.6.1",
    "cssnano": "^4.1.10",
    "sass": "^1.22.10",
    "typescript": "^3.6.2"
  }
}
